algoritmo "semnome"
// Função :
// Autor :
// Data : 25/08/2022
// Seção de Declarações 
var
     valor, parcela, divisao: Real
inicio
      Escreva("Qual é o valor do produto? ")
      Leia(valor)
      Escreva("Quantas parcelas vai dividir? ")
      Leia(parcela)
      divisao <- valor / parcela
      Escreva("O valor ficou", divisao)
// Seção de Comandos 
fimalgoritmo
